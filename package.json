{
  "name": "my-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "build:stg": "cross-env NODE_ENV=\"staging\" BASE_URL=\"https://staging.example.com\" nuxt-ts build",
    "build:prod": "cross-env NODE_ENV=\"production\" BASE_URL=\"https://production.example.com\" nuxt-ts build",
    "start": "nuxt-ts start",
    "generate:stg": "cross-env NODE_ENV=\"staging\" BASE_URL=\"https://staging.example.com\" nuxt-ts generate",
    "generate": "cross-env NODE_ENV=\"production\" BASE_URL=\"https://production.example.com\" nuxt-ts generate",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,scss,css} --ignore-path .gitignore",
    "lint": "yarn lint:js --fix && yarn lint:style --fix"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.12.0",
    "nuxt": "^2.14.0",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@mdi/font-build": "^1.8.5",
    "@nuxt/types": "^2.14.4",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/swiper": "^5.4.0",
    "@types/ua-parser-js": "^0.7.33",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "nuxt-modernizr": "^1.0.2",
    "nuxt-mq": "^2.0.1",
    "nuxt-property-decorator": "^2.7.2",
    "prettier": "^2.0.5",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "swiper": "^6.2.0",
    "ts-loader": "^8.0.3",
    "vue-awesome-swiper": "^4.1.1",
    "vue-scrollto": "^2.18.2"
  }
}
